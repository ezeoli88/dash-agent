{
  "name": "@dash-agent/server",
  "version": "1.0.0",
  "description": "Backend API for managing development tasks where an AI agent executes them autonomously",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "tsc",
    "start": "bun dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:gate": "vitest run --coverage && node scripts/check-coverage.mjs"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "express": "^4.18.2",
    "octokit": "^5.0.5",
    "openai": "^6.17.0",
    "sql.js": "^1.10.3",
    "uuid": "^9.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/sql.js": "^1.4.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "supertest": "^7.2.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
